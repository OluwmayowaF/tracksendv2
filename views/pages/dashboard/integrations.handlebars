	<div class="dashboard-content">

		<!-- Titlebar -->
		<div id="titlebar">
			<div class="row">
				<div class="col-md-12">
					<!-- Breadcrumbs -->
					<nav id="breadcrumbs">
						<ul>
							<li><a href="/dashboard">Dashboard</a></li>
							<li>Integrations</li>
						</ul>
					</nav>
				</div>
			</div>
		</div>

		{{#if flash}}
			<div class="notification {{ flashtype }} closeable no-shadow">
				<p><span style="text-transform: capitalize">{{ flashtype }}!</span> {{ flash }}</p>
				<a class="close"></a>
			</div>
		{{/if}}

			<div class="notification other3 closeable">
				<p></p>
				<a class="close"></a>
			</div>

		<div class="notification success _pk closeable no-shadow list_item" style="border: solid 1px;padding: 10px 20px;">
			<p>API Key: <b class="dv_desc" style="word-break: break-all;">{{ args.apikey }}</b> <i class="fa fa-copy cpy_lnk_btn" style="margin-left: 10px;color:#999; cursor:pointer"></i> <i class="fa fa-refresh" style="margin-left: 10px;color:#999; cursor:pointer"></i></p>
			<a class="close"></a>
		</div>

		<div class="row">

			<div class="toggle-wrap no_borders">

				<div class="col-lg-12 trigger"><div class="col-lg-12 page_box_header toggle active"><i class="im im-icon-Cursor-Click2"></i> SMS <i class="sl sl-icon-plus"></i></div></div>
				<div class="col-lg-12 toggle-container" style="display: none">

					<div id="add-listing">
					<form action="" method="POST">

						<!-- Section -->
						<div class="add-listing-section">

											
							<div class="row with-forms">

								{{!-- <div class="col-md-12">
									<h5>Include Recipients <span>(separate numbers with commas; messages to these won't be tracked.)</span> </h5>
									<input type="text" name="recipients" id="recipients" maxlength="200">
								</div> --}}

								<div class="col-md-12 with-forms" style="margin-top: 20px;">
									<div class="col-md-12 ignore_dnd" style="border: solid 1px #ddd; background-color: #eee; padding-top: 8px; border-radius: 5px">
										<h5 style="float: left">Activate SMS Service  <span>[active]</span></h5>
										<label class="switch"><input type="checkbox" checked="" name="skip_dnd" disabled><span class="slider round"></span></label>
										<div class="clearfix"></div>
									</div>
								</div>

							</div>
							
						</div>
						<!-- Section / End -->

					</form>
					</div>
				</div>

			</div>
			
			<div class="toggle-wrap no_borders">

				<div class="col-lg-12 trigger" style="margin-top: 30px;"><div class="col-lg-12 page_box_header toggle active"><i class="im im-icon-Cursor-Click2"></i> WhatsApp <i class="sl sl-icon-plus"></i></div></div>
				<div class="col-lg-12 toggle-container" style="display: none">

					<div id="add-listing">
					<form action="" method="POST">

						<!-- Section -->
						<div class="add-listing-section">

											
							<div class="row with-forms">

								{{!-- <div class="col-md-12">
									<h5>Include Recipients <span>(separate numbers with commas; messages to these won't be tracked.)</span> </h5>
									<input type="text" name="recipients" id="recipients" maxlength="200">
								</div> --}}
								{{#if args.active}}

								<div class="col-md-12 with-forms" style="margin-top: 20px;">
									<div class="col-md-12 ignore_dnd" style="border: solid 1px #ddd; background-color: #eee; padding-top: 8px; border-radius: 5px">
										<h5 style="float: left">Activate WhatsApp Service <span>[ active ]</span></h5>
										<label class="switch"><input type="checkbox" checked="" name="do_whatsapp" id="do_whatsapp" onchange="javascript: activateWhatsApp(this)"><span class="slider round"></span></label>
										<input type="hidden" name="">
										<div class="clearfix"></div>
									</div>
								</div>

								{{else if args.error }}

								<div class="col-md-12 with-forms" style="margin-top: 20px;">
									<div class="col-md-12 ignore_dnd" style="border: solid 1px #ddd; background-color: #eee; padding-top: 8px; border-radius: 5px">
										<h5 style="float: left">Activate WhatsApp Service <span>[ status unavailable... please check again later ]</span> </h5>
										<input type="hidden" name="">
										<div class="clearfix"></div>
									</div>
								</div>

								<div  class="col-md-12 notification warning" id="pre_activate_whatsapp" style="margin: 20px 0" hidden>
									<span class="">Price: <b>N2,300/month</b>. This would be charged to your account by debiting your Wallet.</span> <button style="margin: 15px 0;" onclick="activateWhatsApp('do')">Proceed</button><span class="loading_icon fa fa-hourglass-2" style="margin: 22px 10px 23px;"></span>

								</div>

								<div class="col-md-12" id="whatsapp_qrcode" style="text-align: center;" hidden>
									{{!-- <img src="data:image/png;base64," alt="Red dot" /> --}}
									{{!-- <img id="img_whatsappqrcode" src="{{ args.qrcode }}" alt="WhatsApp QR Code" /> --}}
									<img id="img_whatsappqrcode" src="" alt="WhatsApp QR Code" />
									</h5><br>
									<em>Here's your QR Code, you need to take a picture of it in your mobile device Whatsapp application <br> by going to Menu -> WhatsApp Web -> Add. <b> <br> QR Code is valid for one minute. Kindly refresh page afterwards.</b></em>
								</div>

								{{else }}

								<div class="col-md-12 with-forms" style="margin-top: 20px;">
									<div class="col-md-12 ignore_dnd" style="border: solid 1px #ddd; background-color: #eee; padding-top: 8px; border-radius: 5px">
										<h5 style="float: left">Activate WhatsApp Service <span>[ inactive ]</span> </h5>
										<label class="switch"><input type="checkbox" name="do_whatsapp" id="do_whatsapp" onchange="activateWhatsApp(this)"><span class="slider round"></span></label>
										<input type="hidden" name="">
										<div class="clearfix"></div>
									</div>
								</div>

								<div  class="col-md-12 notification warning" id="pre_activate_whatsapp" style="margin: 20px 0" hidden>
									<span class="">Price: <b>N2,300/month</b>. This would be charged to your account by debiting your Wallet.</span> <button type="button" style="margin: 15px 0;" onclick="activateWhatsApp('do')">Proceed</button><span class="loading_icon fa fa-hourglass-2" style="margin: 22px 10px 23px;"></span>

								</div>

								<div class="col-md-12" id="whatsapp_qrcode" style="text-align: center;" hidden>
									{{!-- <img src="data:image/png;base64," alt="Red dot" /> --}}
									{{!-- <img id="img_whatsappqrcode" src="{{ args.qrcode }}" alt="WhatsApp QR Code" /> --}}
									<img id="img_whatsappqrcode" src="" alt="WhatsApp QR Code" />
									</h5><br>
									<em>This is a QR Code and you need to take a picture of it in your mobile device Whatsapp application <br> by going to Menu -> WhatsApp Web -> Add. <b> <br> QR Code is valid for one minute.</b></em>
								</div>


								{{/if}}
							</div>
							
						</div>
						<!-- Section / End -->

					</form>
					</div>
				</div>

			</div>

			<div class="col-lg-12" id="show_contacts_pane" style="margin-top: 30px;" hidden>

				<div id="add-listing">
					<!-- Section -->
					<div class="add-listing-section" style="background-color: inherit;">

						<!-- Row --> 
						<div class="row with-forms">
							<br>
							<!-- Status -->
							<div class="col-md-12 lists">
								<h5 style="position: relative"><span id="grp_typ_name"></span></span>Group: <span id="group_name" style="weight: bold"></span> | Contacts: <span class="items_count" id="group_contact_count" style="weight: bold"></span> <i class="sl sl-icon-refresh _loader"></i></h5>
								<div class="input-group mb-3" id="search_conts_box">
									<input type="text" class="form-control" name="search_conts" id="search_conts" placeholder="Search Contacts..." style="width: 92%; display: inline; height: 35px; padding: 10px; font-size: 0.9em;">
									<span class="input-group-append">
										<button class="btn btn-default" id="search_conts_btn">Search</button>
									</span>
								</div>
								<ul id="contact_list">
								</ul>
							</div>

						</div>
						<!-- Row / End -->

					</div>
					<!-- Section / End -->
				</div>
			</div>

			<!-- Copyrights -->
			<div class="col-md-12">
				<div class="copyrights">© 2019 Tracksend. All Rights Reserved.</div>
			</div>

		</div>

	</div>
