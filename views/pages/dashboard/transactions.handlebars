	<div class="dashboard-content">

		<!-- Titlebar -->
		<div id="titlebar">
			<div class="row">
				<div class="col-md-12">
					<!-- Breadcrumbs -->
					<nav id="breadcrumbs">
						<ul>
							<li><a href="/dashboard">Dashboard</a></li>
							<li>Transaction History</li>
						</ul>
					</nav>
				</div>
			</div>
		</div>

		{{#if flash}}
			<div class="notification {{ flashtype }} closeable no-shadow">
				<p><span style="text-transform: capitalize">{{ flashtype }}!</span> {{ flash }}</p>
				<a class="close"></a>
			</div>
		{{/if}}

			<div class="notification other3 closeable">
				<p></p>
				<a class="close"></a>
			</div>

		<div class="row">
			
			<div class="col-lg-12" style="margin-top: 30px">
				<div class="col-lg-12 page_box_header">Transaction History <span>[{{ args.allTransactions.length }}]  </span>
				</div>
			</div>
			<div class="col-lg-12">

				<div id="add-listing">
					<!-- Section -->
					<div class="add-listing-section">

						<!-- Row -->
						<div class="row with-forms">
							<br>
							<!-- Status -->
							<div class="col-md-12 lists">
								<ul>
									<li class="list_item _hd" style="font-weight: bolder">
										<ul class="saved_item">
											<input type="hidden" class="cid" value="'+i.id+'" />
											<li style="flex: 3">Date</li>
											<li style="flex: 3">Amount</li>
											<li style="flex: 3">Units</li>
											<li style="flex: 3">Transaction type</li>
											<li style="flex: 3">Reference number</li>
											<li style="flex: 3">Status</li>

										</ul>
									</li>
									{{#each args.allTransactions}}
									<li class="list_item" data-wh="senderid">
										<ul class="saved_item">
											<input type="hidden" class="cid" value="'+i.id+'" />
											<li style="flex: 3">{{formatDate createdAt "short"}}</li>
											<li style="flex: 3">{{ amount }}</li>
											<li style="flex: 3">{{ units }}</li>
											<li style="flex: 3">{{ description }}</li>
											<li style="flex: 3">{{ trxref }}</li>
											<li style="flex: 3">{{ status }}</li>

										<!--	<li style="flex: 6">{{formatDate createdAt "short"}}</li>
											<li style="flex: 8">{{ amount }}</li>
											<li style="flex: 2">{{ units }}</li>
											<li style="flex: 3">{{ payment.channel }}</li> -->
										</ul>
									</li>
									{{else}}
										<em>No transactions yet.</em> 
									{{/each}}
								</ul>
							</div>

						</div>
						<!-- Row / End -->

					</div>
					<!-- Section / End -->
				</div>
			</div>

			<div class="col-lg-12" style="margin-top: 30px;">

				<div id="add-listing">
				</div>
			</div>

			<!-- Copyrights -->
			<div class="col-md-12">
				<div class="copyrights">Â© 2019 Tracksend. All Rights Reserved.</div>
			</div>

		</div>

	</div>
